import java.util.ArrayList;  
import java.util.List;  
public class Main {  
    public static void main(String[] args) {   // Создаем список и добавляем элементы  
       List<String> k = new ArrayList<>();  
        k.add("s");  
        k.add("g");  
        k.add("v");  
        System.out.println(k.get(0));    // Выводим первый элемент  
        k.add("z");  // Добавляем новый элемент  
        System.out.println(k);   // Выводим весь список  
    }  
}  

	

	
